def solution(arr, divisor):
    answer = []
    
    for i in arr:
        if i%divisor == 0:
            answer.append(i)
    answer.sort()
    if len(answer) == 0:
        return [-1]
    else:
        return answer
        
        -------------------- or
        
        
def solution(arr, divisor): return sorted([n for n in arr if n%divisor == 0]) or [-1]
